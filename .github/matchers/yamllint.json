{
  "problemMatcher": [
    {
      "owner": "yamllint",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s+\\[(warning|error)\\]\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
